{
  "name": "fast-ship-base-code",
  "version": "1.0.0",
  "description": "Universal monorepo template with BMAD workflows for rapid AI-assisted development",
  "private": true,
  "type": "module",
  "scripts": {
    "projects:init": "node scripts/init-project.js",
    "projects:add": "node scripts/add-app.js",
    "create:story": "node scripts/create-story.js",
    "install:all": "pnpm install",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "keywords": [
    "monorepo",
    "template",
    "pnpm",
    "workspace",
    "bmad",
    "ai-assisted",
    "scaffolding",
    "domain-agnostic"
  ],
  "author": "Ardiansyahiqbal",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "devDependencies": {}
}
